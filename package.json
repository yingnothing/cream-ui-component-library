{
  "name": "yingqiu-cream-ui",
  "version": "0.1.4",
  "private": false,
  "type": "module",
  "description": "一个基于Vue 3的现代组件库 Cream-UI",
  "jest": {
    "testEnvironment": "jsdom"
  },
  "keywords": [
    "vue",
    "components",
    "ui",
    "vue3",
    "typescript",
    "cream-ui"
  ],
  "author": "YingQiu",
  "license": "MIT",
"module": "dist/es/index.js",   
  "types": "dist/types/index.d.ts",
  "style": "dist/yingqiu-cream-ui.css",
"exports": {
  ".": {
    "import": ".dist/es/index.js",
    "types": "./dist/types/index.d.ts"
  },
  "./components/Button": {
    "import": "./dist/es/components/Button/index.js"
  },
  "./components/Collapse": {
    "import": "./dist/es/components/Collapse/index.js"
  },
  "./components/Icon": {
    "import": "./dist/es/components/Icon/index.js"
  },
  "./css": {
    "import": "./dist/yingqiu-cream-ui.css"
  },
  "./package.json": "./package.json"
},
  "files": [
    "dist"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "dev": "vite",
  "build": "vite build",
  "preview": "vite preview",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "prepublishOnly": "npm run build",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "dependencies": {
    "lodash-es": "^4.17.21",
    "postcss-each-variables": "^0.3.0"

  },
  "peerDependencies":{
    "@floating-ui/vue": "^1.1.6",
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-brands-svg-icons": "^6.7.2",
    "@fortawesome/free-regular-svg-icons": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/vue-fontawesome": "^3.0.8",
    "@popperjs/core": "^2.11.8",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "vitest": "^3.0.9",

    "@tsconfig/node22": "^22.0.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.13.9",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.22.0",
    "eslint-plugin-vue": "~10.0.0",
    "globals": "^16.0.0",
    "jiti": "^2.4.2",
    "jsdom": "^26.0.0",
    "npm-run-all2": "^7.0.2",
    "postcss": "^8.5.3",
    "postcss-color-mix": "^1.1.0",
    "postcss-each": "^1.1.0",
    "postcss-for": "^2.1.1",
    "postcss-nested": "^7.0.2",
    "prettier": "3.5.3",
    "sass-embedded": "^1.85.1",
    "source-map-explorer": "^2.5.3",
    "terser": "^5.39.0",
    "typescript": "~5.8.0",
    "typescript-eslint": "^8.26.1",
    "vite": "^6.2.1",
    "vite-plugin-dts": "^4.5.3",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vitepress": "^1.6.3",
    "vue-tsc": "^2.2.8",
    "@vitepress-demo-preview/component": "^2.3.2",
    "@vitepress-demo-preview/plugin": "^1.4.0"
  }
